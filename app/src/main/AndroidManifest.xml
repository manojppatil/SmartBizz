            android:screenOrientation="portrait" />
        <activity
            android:name=".WebViewTermsNCondition"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Login"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".SuccessSplash"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".UserProfile"
            android:screenOrientation="sensorPortrait" />

        <receiver android:name=".pqformfragments.SmsReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>

        <service android:name=".FCM.NotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".FCM.GettingDeviceTokenService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".UsageStatistic" />

        <receiver
            android:name=".AlarmReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.NOTIFY" />
            </intent-filter>
        </receiver>

        <service
            android:name=".MyService"
            android:exported="false" />
        <service
            android:name=".MyServiceDemo"
            android:exported="false" />

        <activity
            android:name=".SignupLogin"
            android:screenOrientation="sensorPortrait" />
        /** NEW UI **/

        <activity
            android:name=".SplashScreen"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity

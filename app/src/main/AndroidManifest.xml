    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:name=".MainActivity"
        android:allowBackup="true"
        android:icon="@drawable/eduvanz_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main2ActivityNavigation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImageSlider"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".fqform.borrowerdetail.FqFormBorrower"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fqform.coborrowerdetail.FqFormCoborrower"
            android:screenOrientation="portrait" />
        <activity
            android:name=".uploaddocs.UploadActivityBorrower"
            android:screenOrientation="portrait" />
        <activity
            android:name=".uploaddocs.UploadActivityCoBorrower"
            android:screenOrientation="portrait" />
        <activity
            android:name=".pqformfragments.SuccessAfterPQForm"
            android:screenOrientation="portrait" />
        <activity
            android:name=".friendlyscore.ClientActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".friendlyscore.StartActivityFS"
